{
  "Serilog":{
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.Seq" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "Seq",
        "Args": {"serverUrl": "http://localhost:5341"}
      }
    ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "TeeJayConnection": "Data Source=localhost\\SQLEXPRESS;Database=TeeJayDb;Trust Server Certificate=True;Integrated Security=True;"
  }

}
